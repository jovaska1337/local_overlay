{
	"name": "SearX",
	"description": "Search SearX",
	"manifest_version": 2,
	"version": "1.2",
	"applications": { "gecko": { "id": "searx@search.mozilla.org" } },
	"hidden": true,
	"icons": { "16": "favicon.ico" },
	"web_accessible_resources": [ "favicon.ico" ],
	"chrome_settings_overrides": {
		"search_provider": {
			"keyword": [ "@searx" ],
			"name": "SearX",
			"search_url": "https://search.disroot.org/search",
			"search_form": "https://search.disroot.org/search",
			"search_url_post_params": "q={searchTerms}"
		}
	}
}
